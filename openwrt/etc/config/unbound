config unbound
	option dns64 '0'
	option domain 'lan'
	option edns_size '1280'
	option extended_stats '0'
	option hide_binddata '1'
	option interface_auto '1'
	option listen_port '53'
	option manual_conf '0'
	option num_threads '1'
	option rate_limit '0'
	option rebind_protection '1'
	option recursion 'default'
	option resource 'default'
	option root_age '9'
	option ttl_min '120'
	option validator '0'
	option verbosity '1'
	option localservice '0'
	option enabled '1'
	option protocol 'ip4_only'
	option dhcp_link 'odhcpd'
	option dhcp4_slaac6 '1'
	option domain_type 'static'
	option rebind_localhost '1'
	option unbound_control '1'
	option add_local_fqdn '1'
	option add_wan_fqdn '1'
	list trigger_interface 'lan'
	list trigger_interface 'wan'

config zone
	option enabled '0'
	option url_dir 'https://www.internic.net/domain/'
	option zone_type 'auth_zone'
	list server 'lax.xfr.dns.icann.org'
	list server 'iad.xfr.dns.icann.org'
	list zone_name '.'
	list zone_name 'arpa.'
	list zone_name 'in-addr.arpa.'
	list zone_name 'ip6.arpa.'
	option fallback '0'

config zone
	option zone_type 'forward_zone'
	option enabled '1'
	list server '116.202.176.26'
	list server '1.1.1.1@853#cloudflare-dns.com'
	option dns_assist 'none'
	option tls_upstream '1'
	option tls_port '853'
	option tls_index 'dot.libredns.gr'
	option fallback '0'
	list zone_name '.'
