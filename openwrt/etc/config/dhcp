config dhcp 'lan'
    option interface 'lan'
    option start '100'
    option limit '150'
    option leasetime '12h'
    option dhcpv6 'server'
    option ra 'server'
    option dhcpv4 'server'

config dhcp 'wan'
    option interface 'wan'
    option ignore '1'

config odhcpd 'odhcpd'
    option leasefile '/tmp/hosts/odhcpd'
    option loglevel '4'
    option maindhcp '1'
    option leasetrigger '/usr/lib/unbound/odhcpd.sh'
